{
  "name": "runtime-import",
  "version": "1.0.0",
  "description": "前端资源运行时加载器.",
  "main": "lib/index.js",
  "types": "lib/",
  "scripts": {
    "test": "karma start karma.conf.js",
    "dts": "tsc --emitDeclarationOnly --outDir lib",
    "es5": "rm -rf ./lib && babel ./src -d ./lib --extensions '.ts,.tsx' --source-maps inline && npm run dts",
    "start": "webpack-dev-server --config ./webpack/develop.js --hot --inline",
    "prepublish": "npm run es5",
    "build-debug": "webpack --color --config ./webpack/debug.js",
    "build-release": "webpack --color --config ./webpack/release.js",
    "dep-analyze": "webpack --color --config ./webpack/debug.js --profile --json > ./.package/DependenceNetwork.json",
    "build": "rm -rf ./.package && npm run build-debug && npm run build-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/runtime-import.git"
  },
  "keywords": [
    "import",
    "loader"
  ],
  "author": "Y3G",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yusangeng/runtime-import/issues"
  },
  "homepage": "https://github.com/yusangeng/runtime-import#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.4",
    "mocha": "^5.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {}
}
